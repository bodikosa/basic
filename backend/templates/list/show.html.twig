{% extends 'base.html.twig' %}

{% block title %}ListEntity{% endblock %}

{% block body %}
    <h1>ListEntity</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ list.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ list.name }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ list.value }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ list.type }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ list.createdAt ? list.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ list.updatedAt ? list.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('list_index') }}">back to list</a>

    <a href="{{ path('list_edit', {'id': list.id}) }}">edit</a>

    {{ include('list/_delete_form.html.twig') }}
{% endblock %}
